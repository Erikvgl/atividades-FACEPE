algoritmo "semnome"
var
   nome: caractere
   idade: inteiro
   nota: real
inicio
   escreva("Digite seu nome: ")
   leia(nome)
   
   escreva("Digite sua idade: ")
   leia(idade)
   
   escreva("Digite sua nota:")
   leia(nota)
   
   escreval("")
   
   escrevaL("Seu nome é: ", nome)
   escrevaL("Sua idade é: ", idade)
   escrevaL("Sua nota é: ", nota)
fimalgoritmo